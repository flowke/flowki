{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "awesome app {{packageName}} :) ",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development vad dev",
    "build": "cross-env NODE_ENV=production vad build",
    "preview": "cross-env PREVIEW_MODE=true NODE_ENV=development vad dev"
  },
  "dependencies": {
  {% for key, val in dependencies %}
  {% if loop.last %}
    "{{key}}": "{{val}}"
  {% else %}
    "{{ key }}": "{{val}}",
  {% endif %}
  {% endfor %}

  },
  "devDependencies": {
    {% for key, val in devDependencies %}
    {% if loop.last %}
      "{{key}}": "{{val}}"
    {% else %}
      "{{ key }}": "{{val}}",
    {% endif %}
    {% endfor %}
  }
}